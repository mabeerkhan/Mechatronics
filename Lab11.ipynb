   "source": [
    "from cvzone.SerialModule import SerialObject\n",
    "from time import sleep\n",
    "\n",
    "arduino = SerialObject(\"COM5\")\n",
    "\n",
    "while True:\n",
    "    arduino.sendData([1])\n",
    "    sleep(3)\n",
    "    arduino.sendData([0])\n",
    "    sleep(1)"
